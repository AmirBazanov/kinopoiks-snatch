  "scripts": {
    "typeorm": "ts-node -P libs/typeorm/tsconfig.lib.json ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d typeOrm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d typeOrm.config.ts migration:generate ./migrations/kinopoisk",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/kinopoisk",
    "typeorm:revert-migration": "npm run typeorm -- -d typeOrm.config.ts migration:revert"
  },
